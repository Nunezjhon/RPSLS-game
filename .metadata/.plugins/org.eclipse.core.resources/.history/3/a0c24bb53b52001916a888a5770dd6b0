		p3 = new p3_client();
		
		
	}
	@Test
	void test1() throws Exception{
		
		//test player score input 
		p3.p1Score = 3;
		assert(3 == p3.p1Score);
		
	}
	
	@Test
	void test2() {
		
		p3.playerID = null;
		assert(null == p3.playerID);
		
		p3.playerID = 1;
		assert(p3.playerID == 1);
		
		p3.playerID = 2;
		assert(p3.playerID == 2);
	}

